nhldf <- read.table("/Users/apurvashah/Documents/GitHub/stats10/lab5/NHL_20202021_teamGame.tsv", sep="\t", header=TRUE)
head(nhldf, n=6)
mean(nhldf[ , "shots"])
N <- nrow(nhldf)
cross_ndx <- 1:N + rep( c(1, -1), N/2 )
xshots_allowed <- nhldf[ cross_ndx, "shots"]
mean(xshots_allowed)
xcat_bruns <- c("low", "high")[ as.integer(xshots_allowed >= 33) + 1 ]
shots_table <- table(xcat_bruns)[ c("low", "high") ]
shots_table
# a.
new_tab <- shots_table/sum(shots_table)
#high shots:
new_tab[2]
#b.
new_tab <- shots_table/sum(shots_table)
#low shots:
paste("low shots", new_tab[1], sep = " ")
